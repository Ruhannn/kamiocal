{
  "name": "kamiocal",
  "version": "1.2.0",
  "description": "a easy to use cli tool to share a folder to your local network",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "kamiocal": "dist/index.js",
    "local": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --publicDir",
    "lint": "tsc --noEmit",
    "release": "bun run build && changeset publish dist",
    "change": "bun changeset"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "figlet": "^1.8.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@types/express": "^5.0.0",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.9.3",
    "@types/qrcode-terminal": "^0.12.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}